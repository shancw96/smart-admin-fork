<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="net.lab1024.sa.admin.module.business.recharge.dao.RechargeLogDao">
    <select id="queryByUserId"
            resultType="net.lab1024.sa.admin.module.business.recharge.domain.entity.RechargeLog">
        SELECT *
        FROM t_user_recharge_log
        WHERE user_id = #{userId}
    </select>
</mapper>